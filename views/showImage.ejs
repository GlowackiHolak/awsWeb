<html>
	<head>
		<link href="http://netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css" rel="stylesheet">	
		<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />	
	</head>
	<body>
		<nav class="navbar">
			
		</nav>
		<div class="container">
			<section id="section1" class="panel panel-default">
				<div class="panel-heading">
					<div class="container">
						<h3><strong><%=params.Key%></strong></h3>
					</div>
				</div>
				<div class="panel-body"> 
					
					<img src="https://s3-us-west-2.amazonaws.com/<%=params.Bucket%>/<%=params.Key%>" class="img-thumbnail">
					
					<div class="container">
						<h3><strong>Image details</strong></h3>
					</div>
						
					<table class="table">					
						<tr>
							<th>Key</th><th>User name</th><th>IP address:</th><th>Last modified</th>
						</tr>
						<tr>
							<td><%=params.Key%></td><td><%=fields.Metadata.username%></td><td><%=fields.Metadata.ip%></td><td><%=fields.LastModified%></td>
						</tr>						
					</table>
														
					<div class="container">
						<div class="col-xs-2">
							<button type="button" class="btn  btn-primary btn-block" onClick="window.location.href='/imageList'">Return</button>
						</div>
						<div class="col-xs-2">
							<button type="button" class="btn  btn-primary btn-block" onClick="window.location.href='/'">Home Page</button>
						</div>
					</div>
				</div>
	  			<footer class="panel-footer">
				
				</footer>
			</section>
		</div>		
	</body>
</html>
